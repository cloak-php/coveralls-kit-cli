{
    "name": "cloak/coverallskit-cli",
    "type": "library",
    "description": "Command line interface for coverallskit",
    "version": "1.0.0",
    "keywords": [
        "cli", "command line", "coveralls", "code coverage", "report"
    ],
    "bin": [ "bin/coverallskit" ],
    "require": {
        "php": ">=5.5.0",
        "zendframework/zend-console": "2.3.*",
        "phpcollection/phpcollection": "0.4.*",
        "cloak/coverallskit": "1.3.*"
    },
    "require-dev": {
        "danielstjules/pho": "dev-master",
        "jaz303/phake": "0.6.*",
        "phpspec/prophecy": "1.2.*",
        "phpunit/php-code-coverage": "3.0.*@dev",
        "cloak/lcov-reporter": "1.1.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Noritaka Horio",
            "email": "holy.shared.design@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "coverallskit\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "coverallskit\\spec\\fixture\\": "spec/fixtures/" }
    }
}
