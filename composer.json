{
    "name": "cloak/coverallskit-cli",
    "type": "library",
    "description": "Command line interface for coverallskit",
    "version": "1.0.0",
    "keywords": [
        "cli", "command line", "coveralls", "code coverage", "report"
    ],
    "bin": [
        "bin/coverallskit"
    ],
    "require": {
        "php": ">=5.5.0",
        "aura/cli": "2.*",
        "aura/cli-kernel": "2.0.*@dev",
        "aura/project-kernel": "2.0.*@dev",
        "aura/di": "2.*@dev",
        "cloak/coverallskit": "1.3.*",
        "monolog/monolog": "1.*"
    },
    "require-dev": {
        "danielstjules/pho": "dev-master",
        "jaz303/phake": "0.6.*",
        "phpspec/prophecy": "1.2.*",
        "cloak/lcov-reporter": "1.1.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Noritaka Horio",
            "email": "holy.shared.design@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "coverallskit\\": "src/",
            "Aura\\Cli_Project\\_Config\\": "config/"
        }
    },
    "extra": {
        "aura": {
            "type": "project",
            "config": {
                "common": "Aura\\Cli_Project\\_Config\\Common",
                "dev": "Aura\\Cli_Project\\_Config\\Dev",
                "test": "Aura\\Cli_Project\\_Config\\Test",
                "prod": "Aura\\Cli_Project\\_Config\\Prod"
            }
        }
    }
}
